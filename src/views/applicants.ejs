<h2 class="heading-applicant">Applicant List</h2>
<table class="table table-dark">
   
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Contact</th>
      <th scope="col">Resume</th>
    </tr>
  </thead>
  <tbody>
    <% applicant.forEach(applicant =>{ %>
      <tr>
        <th scope="row"><%= applicant.id %></th>
        <td><%= applicant.applicantname%></td>
        <td><%= applicant.emailid %></td>
        <td><%= applicant.applicantcontact %></td>
        <td>
          <a href="<%= applicant.resume %>" target="_blank" name="resume" class="btn btn-outline-primary">View</a>
        </td>
      </tr>
   <% }); %>


  </tbody>
</table>
